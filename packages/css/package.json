{
  "name": "@jabardigitalservice/jds-design-system-css",
  "version": "1.0.0",
  "description": "A starter template for building websites using Sass.",
  "main": "index.js",
  "scripts": {
    "start": "(rm -rf css || 1) && sass --style=expanded scss:css && autoprefixer-cli --no-map -o 'css/index.css' 'css/index.css'",
    "watch": "nodemon -e scss -x 'npm run start'"
  },
  "keywords": [
    "css",
    "sass"
  ],
  "author": "Josh Wenning",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "nodemon": "^1.8.1",
    "sass": "^1.34.1"
  },
  "dependencies": {},
  "files": [
    "css",
    "scss"
  ]
}
